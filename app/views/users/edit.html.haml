- if current_user.admin?
  .row
    .columns.large-10.large-offset-1
      .breadcrumb
        = link_to 'Users', users_path

.row
  .columns.large-10.large-offset-1
    %fieldset.mt--
      = form_for(@user) do |form|
        = render form
        = form.submit "Submit", class: 'button small no-margin right submit radius'
